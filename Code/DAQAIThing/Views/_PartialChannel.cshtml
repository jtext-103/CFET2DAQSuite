<div class="panel panel-default" id="@Model partialId">
    <div class="panel-heading">
        <h2 class="panel-title">
            <font color="RoyalBlue">
                Channel @Model.ToString()
            </font>
            &nbsp;&nbsp;
            <font id="@Model sampleId"></font>
            &nbsp;&nbsp;
            <font id="@Model totalId"></font>
        </h2>
    </div>
    <div class="panel-body">
        <svg id="@Model.ToString() svgId"></svg>
    </div>
    <div class="panel-body row">
        <div class="row" style="margin-left: 0px">
            <div class="col-md-2">
                <div class="input-group">
                    <span class="input-group-addon">Start Point</span>
                    <input id="@Model.ToString()startId" type="text" class="form-control" value="0">
                </div>
            </div>
            <div class="col-md-2">
                <div class="input-group">
                    <span class="input-group-addon">End Point</span>
                    <input id="@Model.ToString()endId" type="text" class="form-control" value="0">
                </div>
            </div>
            <div class="col-md-2">
                <div class="input-group">
                    <span class="input-group-addon">Shot No</span>
                    <input id="@Model.ToString()partialShotId" type="text" class="form-control" value="0">
                </div>
            </div>
            <div class="col-md-2">
                <button type="button" class="btn btn-primary btn-mid" onclick="load(@Model, true);">
                    <b>&nbsp;Load&nbsp;</b>
                    <span class="glyphicon glyphicon-save"></span>
                </button>
            </div>
        </div>
    </div>
</div>